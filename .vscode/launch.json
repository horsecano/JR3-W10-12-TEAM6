{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Attach",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/${input:project}/build/kernel.o",
      "MIMode": "gdb",
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "targetArchitecture": "x86_64",
      "externalConsole": true,
      "miDebuggerServerAddress": "localhost:1234",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "dbg_pintos"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ubuntu/pintos-kaist/threads",
      "program": "/home/ubuntu/pintos-kaist/threads/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ],
  "inputs": [
    {
      "id": "project",
      "description": "Enter the project to start.",
      "type": "pickString",
      "options": [
        "threads",
        "userprog",
        "filesys",
        "vm"
      ]
    }
  ]
}